{
  "global": {
    "size": {
      "breakpoint-min-width": {
        "$type": "sizing",
        "$value": "{primitives.size.breakpoint.0}"
      }
    },
    "space": {
      "macroScale": {
        "$type": "spacing",
        "$value": "0.8",
        "$description": "Spacing scale for small screens."
      }
    }
  },
  "page": {
    "space": {
      "x": {
        "L": {
          "$type": "spacing",
          "$value": "{global.space.macro.S}"
        }
      },
      "y": {
        "L": {
          "$type": "spacing",
          "$value": "{global.space.macro.S}"
        }
      }
    }
  }
}