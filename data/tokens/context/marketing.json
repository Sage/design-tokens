{
  "button": {
    "color": {
      "typical": {
        "toggle": {
          "label-active": {
            "$type": "color",
            "$value": "{modes.color.interactive.monochrome.marketing.withActive}"
          }
        },
        "tertiary": {
          "bg-enabled": {
            "$type": "color",
            "$value": "{modes.color.none}"
          }
        }
      },
      "video": {
        "primary": {
          "bg-enabled": {
            "$type": "color",
            "$value": "{modes.color.interactive.monochrome.marketing.withActive}"
          },
          "bg-hover": {
            "$type": "color",
            "$value": "{modes.color.interactive.monochrome.marketing.withActive}"
          },
          "label-enabled": {
            "$type": "color",
            "$value": "{modes.color.interactive.monochrome.default}"
          },
          "label-hover": {
            "$type": "color",
            "$value": "{modes.color.interactive.monochrome.default}"
          }
        }
      }
    },
    "borderwidth": {
      "tertiary": {
        "$type": "borderWidth",
        "$value": "{global.borderwidth.none}",
        "$description": "resetting tertiary border to 0px"
      },
      "tertiarymarketing": {
        "$type": "borderWidth",
        "$value": "{global.borderwidth.S}",
        "$description": "tertiary button border width override"
      }
    },
    "space": {
      "tertiary": {
        "x": {
          "S": {
            "$type": "spacing",
            "$value": "{global.space.none}",
            "$description": "Left and right padding on small buttons"
          },
          "M": {
            "$type": "spacing",
            "$value": "{global.space.none}",
            "$description": "Left and right padding on medium buttons"
          },
          "L": {
            "$type": "spacing",
            "$value": "{global.space.none}",
            "$description": "Left and right padding on large buttons"
          }
        },
        "icononly": {
          "x": {
            "S": {
              "$type": "spacing",
              "$value": "{global.space.none}",
              "$description": "Left and right padding on icon-only processing button "
            },
            "M": {
              "$type": "spacing",
              "$value": "{global.space.none}",
              "$description": "Left and right padding on icon-only processing button "
            },
            "L": {
              "$type": "spacing",
              "$value": "{global.space.none}",
              "$description": "Left and right padding on icon-only processing button "
            }
          }
        }
      }
    },
    "radius": {
      "tertiary": {
        "S": {
          "$type": "borderRadius",
          "$value": "{global.radius.none}",
          "$description": "tertiary button"
        },
        "M": {
          "$type": "borderRadius",
          "$value": "{global.radius.none}",
          "$description": "tertiary button"
        },
        "L": {
          "$type": "borderRadius",
          "$value": "{global.radius.none}",
          "$description": "tertiary button"
        }
      }
    }
  },
  "form": {
    "color": {
      "switch": {
        "fg-active": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.marketing.withActive}"
        },
        "bg-active": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.marketing.active}"
        }
      }
    }
  },
  "tab": {
    "color": {
      "border-active": {
        "$type": "color",
        "$value": "{modes.color.interactive.monochrome.marketing.active}"
      }
    }
  },
  "container": {
    "radius": {
      "card": {
        "moderate": {
          "$type": "borderRadius",
          "$value": "{global.radius.container.XXL}",
          "$description": "Card (less rounded)"
        },
        "curved": {
          "$type": "borderRadius",
          "$value": "{global.radius.container.XXXL}",
          "$description": "Card (more rounded)"
        },
        "sweeping": {
          "$type": "borderRadius",
          "$value": "{global.radius.container.XXXXL}",
          "$description": "Card largest rounded type"
        }
      },
      "tile": {
        "moderate": {
          "$type": "borderRadius",
          "$value": "{global.radius.container.XXL}",
          "$description": "Tile (less rounded)"
        },
        "curved": {
          "$type": "borderRadius",
          "$value": "{global.radius.container.XXXL}",
          "$description": "Tile (more rounded)"
        },
        "sweeping": {
          "$type": "borderRadius",
          "$value": "{global.radius.container.XXXXL}",
          "$description": "Tile largest roundedness "
        }
      },
      "tileselect": {
        "moderate": {
          "$type": "borderRadius",
          "$value": "{global.radius.container.XXL}",
          "$description": "Tile (less rounded)"
        },
        "curved": {
          "$type": "borderRadius",
          "$value": "{global.radius.container.XXXL}",
          "$description": "Tile (more rounded)"
        },
        "sweeping": {
          "$type": "borderRadius",
          "$value": "{global.radius.container.XXXXL}"
        }
      }
    }
  },
