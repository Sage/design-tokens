{
  "link": {
    "color": {
      "destructive": {
        "label-default": {
          "$type": "color",
          "$value": "{modes.color.interactive.danger.defaultAlt}",
          "$description": "."
        },
        "label-hover": {
          "$type": "color",
          "$value": "{modes.color.interactive.danger.hoverAlt2}"
        }
      },
      "typical": {
        "label-default": {
          "$type": "color",
          "$value": "{modes.color.interactive.primary.defaultAlt}"
        },
        "label-hover": {
          "$type": "color",
          "$value": "{modes.color.interactive.primary.hoverAlt}"
        }
      },
      "subtle": {
        "label-default": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.default}"
        },
        "label-hover": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.active}"
        }
      }
    },
    "size": {
      "skiplink": {
        "M": {
          "$type": "sizing",
          "$value": "{global.size.macro.M}"
        }
      },
      "icon": {
        "M": {
          "$type": "sizing",
          "$value": "{global.size.icon.M}"
        }
      }
    },
    "radius": {
      "link": {
        "$type": "borderRadius",
        "$value": "{global.radius.interactive.XS}",
        "$description": "Link (focus bg and bottom corners of focus underline)"
      },
      "skiplink": {
        "$type": "borderRadius",
        "$value": "{global.radius.interactive.M}",
        "$description": "Skiplink (right corners of focus bg and border)"
      }
    },
    "space": {
      "integral": {
        "xg": {
          "M": {
            "$type": "spacing",
            "$value": "{global.space.micro.L}",
            "$description": "Space between icon and link text."
          },
          "L": {
            "$type": "spacing",
            "$value": "{global.space.micro.L}",
            "$description": "Space between icon and link text."
          }
        }
      },
      "skiplink": {
        "x": {
          "$type": "spacing",
          "$value": "{global.space.macro.M}",
          "$description": "Skiplink container"
        },
        "y": {
          "$type": "spacing",
          "$value": "{global.space.micro.XXL}",
          "$description": "Skiplink container"
        }
      }
    },
    "boxshadow": {
      "skiplink": {
        "$type": "boxShadow",
        "$value": "{global.boxshadow.container.near}"
      }
    },
    "typography": {
      "responsive": {
        "default": {
          "S": {
            "$type": "typography",
            "$value": "{global.typography.responsive.component.underlined.moderate.S}",
            "$description": "Small Viewports: 14.81, Large Viewports: 13.79. Match token size to component size."
          },
          "M": {
            "$type": "typography",
            "$value": "{global.typography.responsive.component.underlined.moderate.M}",
            "$description": "Small Viewports: 16, Large Viewports: 16. Match token size to component size."
          },
          "L": {
            "$type": "typography",
            "$value": "{global.typography.responsive.component.underlined.moderate.L}",
            "$description": "Small Viewports: 17.28, Large Viewports: 18.56. Match token size to component size."
          }
        },
        "heading": {
          "S": {
            "$type": "typography",
            "$value": "{global.typography.responsive.component.underlined.firm.S}"
          },
          "M": {
            "$type": "typography",
            "$value": "{global.typography.responsive.component.underlined.firm.M}"
          },
          "L": {
            "$type": "typography",
            "$value": "{global.typography.responsive.component.underlined.firm.L}"
          }
        }
      },
      "adaptive": {
        "default": {
          "S": {
            "$type": "typography",
            "$value": "{global.typography.adaptive.component.underlined.moderate.S}"
          },
          "M": {
            "$type": "typography",
            "$value": "{global.typography.adaptive.component.underlined.moderate.M}"
          },
          "L": {
            "$type": "typography",
            "$value": "{global.typography.adaptive.component.underlined.moderate.L}"
          }
        },
        "heading": {
          "S": {
            "$type": "typography",
            "$value": "{global.typography.adaptive.component.underlined.firm.S}"
          },
          "M": {
            "$type": "typography",
            "$value": "{global.typography.adaptive.component.underlined.firm.M}"
          },
          "L": {
            "$type": "typography",
            "$value": "{global.typography.adaptive.component.underlined.firm.L}"
          }
        }
      }
    }
  }
}