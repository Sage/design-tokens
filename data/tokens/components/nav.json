{
  "nav": {
    "color": {
      "bg-default": {
        "$type": "color",
        "$value": "{modes.color.generic.bg.nought}",
        "$description": "nav bar bg"
      },
      "item": {
        "bg-active": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.active}"
        },
        "bg-activealt": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.hoverAlt}"
        },
        "bg-default": {
          "$type": "color",
          "$value": "{modes.color.none}"
        },
        "bg-hover": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.hoverAlt}"
        },
        "label-active": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.withActive}"
        },
        "label-activealt": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.withHover}"
        },
        "label-default": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.default}"
        },
        "label-hover": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.withHover}"
        },
        "label-alt": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.defaultAlt}"
        }
      },
      "menu": {
        "bg-default": {
          "$type": "color",
          "$value": "{modes.color.generic.bg.faint}",
          "$description": "menu (level 1) nav bg"
        },
        "bg-alt": {
          "$type": "color",
          "$value": "{modes.color.generic.bg.delicate}",
          "$description": "level 2 nav bg"
        },
        "bg-alt2": {
          "$type": "color",
          "$value": "{modes.color.generic.bg.soft}",
          "$description": "level 3 nav bg"
        },
        "border-default": {
          "$type": "color",
          "$value": "{modes.color.generic.fg.delicate}",
          "$description": "level 1 border"
        }
      },
      "leftnav": {
        "container": {
          "bg-default": {
            "$type": "color",
            "$value": "{modes.color.generic.bg.inverse.nought}",
            "$description": "bg of horizontal nav variant that launches the left nav"
          },
          "bg-defaultAlt": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.inverse.defaultAlt}",
            "$description": "bg of the parent container"
          },
          "border-default": {
            "$type": "color",
            "$value": "{modes.color.generic.fg.inverse.soft}",
            "$description": "dividing lines within parent container"
          }
        },
        "item": {
          "bg-active": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.inverse.active}"
          },
          "bg-default": {
            "$type": "color",
            "$value": "{modes.color.none}"
          },
          "bg-hover": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.inverse.hover}"
          },
          "label-active": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.inverse.withActive}"
          },
          "label-default": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.inverse.withDefault}"
          },
          "label-hover": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.inverse.withHover}"
          }
        }
      },
      "modal": {
        "container": {
          "bg-default": {
            "$type": "color",
            "$value": "{modes.color.generic.bg.nought}",
            "$description": "bg of container holding stack of items in modal nav"
          },
          "bg-defaultAlt": {
            "$type": "color",
            "$value": "{modes.color.generic.backdrop.faint}",
            "$description": "bg of the sticky header within modal"
          },
          "border-default": {
            "$type": "color",
            "$value": "{modes.color.generic.fg.soft}",
            "$description": "dividing lines within parent container"
          }
        },
        "item": {
          "bg-active": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.active}"
          },
          "bg-default": {
            "$type": "color",
            "$value": "{modes.color.none}"
          },
          "bg-hover": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.hover}"
          },
          "label-active": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.withActive}"
          },
          "label-default": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.withDefault}"
          },
          "label-hover": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.withHover}"
          }
        },
        "headeritem": {
          "label-default": {
            "$type": "color",
            "$value": "{modes.color.generic.content.firm}",
            "$description": "color for subheaders passed into modal nav"
          }
        }
      },
      "topnav": {
        "container": {
          "bg-default": {
            "$type": "color",
            "$value": "{modes.color.generic.bg.nought}",
            "$description": "bg of of horizontal nav variant that launches the left nav"
          },
          "border-default": {
            "$type": "color",
            "$value": "{modes.color.generic.fg.soft}",
            "$description": "dividing lines within parent container"
          }
        },
        "item": {
          "bg-active": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.active}"
          },
          "bg-default": {
            "$type": "color",
            "$value": "{modes.color.none}"
          },
          "bg-hover": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.hover}"
          },
          "label-active": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.withActive}"
          },
          "label-default": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.withDefault}"
          },
          "label-hover": {
            "$type": "color",
            "$value": "{modes.color.interactive.nav.withHover}"
          }
        }
      }
    },
    "boxshadow": {
      "menu": {
        "$type": "boxShadow",
        "$value": "{global.boxshadow.cleanedge.near}",
        "$description": "Menu"
      },
      "leftnav": {
        "childcontainer": {
          "$type": "boxShadow",
          "$value": "{global.boxshadow.container.near}",
          "$description": "box shadow on progressively shown child container"
        }
      }
    },
    "size": {
      "menu": {
        "content": {
          "y": {
            "M": {
              "$type": "sizing",
              "$value": "{global.size.macro.S}",
              "$description": "Min height of menu content"
            }
          },
          "icon": {
            "M": {
              "$type": "sizing",
              "$value": "{global.size.icon.M}",
              "$description": "Icon size"
            },
            "L": {
              "$type": "sizing",
              "$value": "{global.size.icon.L}",
              "$description": "Icon size"
            }
          }
        },
        "option": {
          "x": {
            "M": {
              "$type": "sizing",
              "$value": "{global.size.flex.M}",
              "$description": "Min width of options"
            }
          },
          "y": {
            "M": {
              "$type": "sizing",
              "$value": "{global.size.macro.M}",
              "$description": "Min height of menu option"
            }
          }
        }
      },
      "navbar": {
        "content": {
          "y": {
            "M": {
              "$type": "sizing",
              "$value": "{global.size.macro.S}",
              "$description": "Min height of nav content"
            }
          },
          "icon": {
            "M": {
              "$type": "sizing",
              "$value": "{global.size.icon.M}",
              "$description": "Chevron and caret icons"
            },
            "L": {
              "$type": "sizing",
              "$value": "{global.size.icon.L}",
              "$description": "Icon size"
            }
          }
        },
        "item": {
          "x": {
            "M": {
              "$type": "sizing",
              "$value": "{global.size.flex.M}",
              "$description": "Max width of nav item"
            }
          },
          "y": {
            "M": {
              "$type": "sizing",
              "$value": "{global.size.macro.M}",
              "$description": "Min height of nav item"
            }
          }
        }
      },
      "leftnav": {
        "parent": {
          "minheight": {
            "$type": "sizing",
            "$value": "{global.size.macro.L}",
            "$description": "Min height of parent items"
          }
        },
        "child": {
          "minheight": {
            "$type": "sizing",
            "$value": "{global.size.macro.M}",
            "$description": "Min height of children items"
          }
        },
        "grandchild": {
          "minheight": {
            "$type": "sizing",
            "$value": "{global.size.macro.S}",
            "$description": "Min height of grandchildren"
          }
        },
        "parentContainer": {
          "fixedWidth": {
            "$type": "sizing",
            "$value": "146",
            "$description": "fixed width of parent container"
          }
        },
        "childContainer": {
          "fixedWidth": {
            "$type": "sizing",
            "$value": "250",
            "$description": "fixed width of parent container"
          }
        }
      },
      "modal": {
        "parentitem": {
          "minheight": {
            "$type": "sizing",
            "$value": "{global.size.macro.M}",
            "$description": "Min height of parent items"
          }
        },
        "childitem": {
          "minheight": {
            "$type": "sizing",
            "$value": "{global.size.macro.S}",
            "$description": "Min height of children items"
          }
        },
        "headeritem": {
          "identifier": {
            "M": {
              "$type": "sizing",
              "$value": "{global.size.icon.L}",
              "$description": "Icon size"
            }
          },
          "icon": {
            "M": {
              "$type": "sizing",
              "$value": "{global.size.icon.M}",
              "$description": "Icon size"
            }
          }
        }
      },
      "topnav": {
        "parent": {
          "minheight": {
            "$type": "sizing",
            "$value": "{global.size.macro.S}",
            "$description": "Min height of parent items across items in secondary top nav "
          }
        },
        "container": {
          "minheight": {
            "$type": "sizing",
            "$value": "{global.size.macro.M}",
            "$description": "Min height of secondary nav containers"
          }
        }
      }
    }
  }
}