{
  "nav": {
    "bg-default": {
      "$type": "color",
      "$value": "{modes.color.generic.bg.nought}",
      "$description": "nav bar bg"
    },
    "leftnav": {
      "container": {
        "bg-default": {
          "$type": "color",
          "$value": "{modes.color.generic.bg.inv.nought}",
          "$description": "bg of horizontal nav variant that launches the left nav"
        },
        "bg-defaultAlt": {
          "$type": "color",
          "$value": "{modes.color.action.nav.inv.defaultAlt}",
          "$description": "bg of the parent container"
        },
        "border-default": {
          "$type": "color",
          "$value": "{modes.color.generic.fg.inv.soft}",
          "$description": "dividing lines within parent container"
        }
      },
      "item": {
        "bg-actv": {
          "$type": "color",
          "$value": "{modes.color.action.nav.inv.actv}"
        },
        "bg-default": {
          "$type": "color",
          "$value": "{modes.color.none}"
        },
        "bg-hov": {
          "$type": "color",
          "$value": "{modes.color.action.nav.inv.hov}"
        },
        "label-actv": {
          "$type": "color",
          "$value": "{modes.color.action.nav.inv.withActv}"
        },
        "label-default": {
          "$type": "color",
          "$value": "{modes.color.action.nav.inv.withDefault}"
        },
        "label-hov": {
          "$type": "color",
          "$value": "{modes.color.action.nav.inv.withhov}"
        }
      }
    },
    "modal": {
      "container": {
        "bg-default": {
          "$type": "color",
          "$value": "{modes.color.generic.bg.nought}",
          "$description": "bg of container holding stack of items in modal nav"
        },
        "bg-defaultAlt": {
          "$type": "color",
          "$value": "{modes.color.generic.surface.faint}",
          "$description": "bg of the sticky header within modal"
        },
        "border-default": {
          "$type": "color",
          "$value": "{modes.color.generic.fg.soft}",
          "$description": "dividing lines within parent container"
        }
      },
      "item": {
        "bg-actv": {
          "$type": "color",
          "$value": "{modes.color.action.nav.actv}"
        },
        "bg-default": {
          "$type": "color",
          "$value": "{modes.color.none}"
        },
        "bg-hov": {
          "$type": "color",
          "$value": "{modes.color.action.nav.hov}"
        },
        "label-actv": {
          "$type": "color",
          "$value": "{modes.color.action.nav.withActv}"
        },
        "label-default": {
          "$type": "color",
          "$value": "{modes.color.action.nav.withDefault}"
        },
        "label-hov": {
          "$type": "color",
          "$value": "{modes.color.action.nav.withhov}"
        }
      },
      "headeritem": {
        "label-default": {
          "$type": "color",
          "$value": "{modes.color.generic.txt.firm}",
          "$description": "color for subheaders passed into modal nav"
        }
      }
    },
    "topnav": {
      "container": {
        "bg-default": {
          "$type": "color",
          "$value": "{modes.color.generic.bg.nought}",
          "$description": "bg of of horizontal nav variant that launches the left nav"
        },
        "border-default": {
          "$type": "color",
          "$value": "{modes.color.generic.fg.soft}",
          "$description": "dividing lines within parent container"
        }
      },
      "item": {
        "bg-actv": {
          "$type": "color",
          "$value": "{modes.color.action.nav.actv}"
        },
        "bg-default": {
          "$type": "color",
          "$value": "{modes.color.none}"
        },
        "bg-hov": {
          "$type": "color",
          "$value": "{modes.color.action.nav.hov}"
        },
        "label-actv": {
          "$type": "color",
          "$value": "{modes.color.action.nav.withActv}"
        },
        "label-default": {
          "$type": "color",
          "$value": "{modes.color.action.nav.withDefault}"
        },
        "label-hov": {
          "$type": "color",
          "$value": "{modes.color.action.nav.withhov}"
        }
      }
    }
  }
}