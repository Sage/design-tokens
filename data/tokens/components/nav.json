{
  "nav": {
    "bg-default": {
      "$type": "color",
      "$value": "{mode.color.generic.bg.nought}",
      "$description": "nav bar bg"
    },
    "leftnav": {
      "container": {
        "bg-default": {
          "$type": "color",
          "$value": "{mode.color.generic.bg.inverse.nought}",
          "$description": "bg of horizontal nav variant that launches the left nav"
        },
        "bg-defaultAlt": {
          "$type": "color",
          "$value": "{mode.color.action.nav.inverse.defaultAlt}",
          "$description": "bg of the parent container"
        },
        "border-default": {
          "$type": "color",
          "$value": "{mode.color.generic.fg.inverse.firm}",
          "$description": "dividing lines within parent container"
        }
      },
      "item": {
        "bg-active": {
          "$type": "color",
          "$value": "{mode.color.action.nav.inverse.active}"
        },
        "bg-default": {
          "$type": "color",
          "$value": "{mode.color.none}"
        },
        "bg-hover": {
          "$type": "color",
          "$value": "{mode.color.action.nav.inverse.hover}"
        },
        "label-active": {
          "$type": "color",
          "$value": "{mode.color.action.nav.inverse.withActive}"
        },
        "label-default": {
          "$type": "color",
          "$value": "{mode.color.action.nav.inverse.withDefault}"
        },
        "label-hover": {
          "$type": "color",
          "$value": "{mode.color.action.nav.inverse.withhover}"
        }
      }
    },
    "modal": {
      "container": {
        "bg-default": {
          "$type": "color",
          "$value": "{mode.color.generic.bg.nought}",
          "$description": "bg of container holding stack of items in modal nav"
        },
        "bg-defaultAlt": {
          "$type": "color",
          "$value": "{mode.color.generic.surface.faint}",
          "$description": "bg of the sticky header within modal"
        },
        "border-default": {
          "$type": "color",
          "$value": "{mode.color.generic.fg.firm}",
          "$description": "dividing lines within parent container"
        }
      },
      "item": {
        "bg-active": {
          "$type": "color",
          "$value": "{mode.color.action.nav.active}"
        },
        "bg-default": {
          "$type": "color",
          "$value": "{mode.color.none}"
        },
        "bg-hover": {
          "$type": "color",
          "$value": "{mode.color.action.nav.hover}"
        },
        "label-active": {
          "$type": "color",
          "$value": "{mode.color.action.nav.withActive}"
        },
        "label-default": {
          "$type": "color",
          "$value": "{mode.color.action.nav.withDefault}"
        },
        "label-hover": {
          "$type": "color",
          "$value": "{mode.color.action.nav.withhover}"
        }
      },
      "headeritem": {
        "label-default": {
          "$type": "color",
          "$value": "{mode.color.generic.txt.soft}",
          "$description": "color for subheaders passed into modal nav"
        }
      }
    },
    "topnav": {
      "container": {
        "bg-default": {
          "$type": "color",
          "$value": "{mode.color.generic.bg.nought}",
          "$description": "bg of of horizontal nav variant that launches the left nav"
        },
        "border-default": {
          "$type": "color",
          "$value": "{mode.color.generic.fg.firm}",
          "$description": "dividing lines within parent container"
        }
      },
      "item": {
        "bg-active": {
          "$type": "color",
          "$value": "{mode.color.action.nav.active}"
        },
        "bg-default": {
          "$type": "color",
          "$value": "{mode.color.none}"
        },
        "bg-hover": {
          "$type": "color",
          "$value": "{mode.color.action.nav.hover}"
        },
        "label-active": {
          "$type": "color",
          "$value": "{mode.color.action.nav.withActive}"
        },
        "label-default": {
          "$type": "color",
          "$value": "{mode.color.action.nav.withDefault}"
        },
        "label-hover": {
          "$type": "color",
          "$value": "{mode.color.action.nav.withhover}"
        }
      }
    }
  }
}