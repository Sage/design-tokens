{
  "logo": {
    "color": {
      "sage": {
        "bg-default": {
          "$type": "color",
          "$value": "{modes.color.brand.default}",
          "$description": "sage logo"
        },
        "bg-alt": {
          "$type": "color",
          "$value": "{modes.color.brand.defaultAlt}"
        }
      },
      "trust": {
        "bg-default": {
          "$type": "color",
          "$value": "{modes.color.status.positive.default}",
          "$description": "filled"
        },
        "outline": {
          "$type": "color",
          "$value": "{modes.color.generic.content.extreme}"
        }
      },
      "copilot": {
        "noBg": {
          "fg-default": {
            "$type": "color",
            "$value": "{modes.color.brand.copilot.noBg.monochrome}"
          }
        }
      },
      "AIIdentifier": {
        "bg-star": {
          "$type": "color",
          "$value": "{modes.color.brand.copilot.AIIdentifier.default}",
          "$description": "star bg color"
        },
        "bg-dot": {
          "$type": "color",
          "$value": "{modes.color.brand.copilot.AIIdentifier.withDefault}"
        },
        "outline": {
          "$type": "color",
          "$value": "{modes.color.brand.copilot.AIIdentifier.default}",
          "$description": "star outline variant"
        }
      }
    },
    "size": {
      "AI": {
        "M": {
          "$type": "sizing",
          "$value": "14px",
          "$description": "AI star"
        }
      }
    },
    "space": {
      "AI": {
        "xg": {
          "M": {
            "$type": "spacing",
            "$value": "{global.space.micro.L}",
            "$description": "Space between star icon and text"
          }
        }
      }
    }
  }
}