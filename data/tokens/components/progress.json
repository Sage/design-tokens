{
  "progress": {
    "size": {
      "bar": {
        "S": {
          "$type": "sizing",
          "$value": "{global.size.micro.XS}",
          "$description": "S loader bar"
        },
        "M": {
          "$type": "sizing",
          "$value": "{global.size.micro.M}",
          "$description": "M progress tracker bar, M loader bar"
        },
        "L": {
          "$type": "sizing",
          "$value": "{global.size.micro.XL}",
          "$description": "L loader bar"
        }
      },
      "carouselselector": {
        "dot": {
          "M": {
            "$type": "sizing",
            "$value": "{global.size.micro.L}"
          }
        },
        "activedot": {
          "M": {
            "$type": "sizing",
            "$value": "{global.size.macro.XXS}"
          }
        },
        "hitarea": {
          "M": {
            "$type": "sizing",
            "$value": "{global.size.macro.XS}"
          }
        }
      },
      "carouselslide": {
        "x": {
          "M": {
            "$type": "sizing",
            "$value": "{global.size.macro.XL}",
            "$description": "Inactive slide"
          }
        }
      },
      "spinner": {
        "XXS": {
          "$type": "sizing",
          "$value": "{global.size.macro.XXS}",
          "$description": "Loader ring for XXS size loader spinners"
        },
        "XS": {
          "$type": "sizing",
          "$value": "{global.size.macro.XS} - {global.size.micro.XS}",
          "$description": "Loader ring for XS size loader spinners"
        },
        "S": {
          "$type": "sizing",
          "$value": "{global.size.macro.S}",
          "$description": "Loader ring"
        },
        "M": {
          "$type": "sizing",
          "$value": "{global.size.macro.S} * 2",
          "$description": "Loader ring"
        },
        "L": {
          "$type": "sizing",
          "$value": "{global.size.macro.XS} * 4",
          "$description": "Loader ring"
        }
      },
      "skeleton": {
        "responsive": {
          "text": {
            "XS": {
              "$type": "sizing",
              "$value": "{core.fontSize.fluid.step0}",
              "$description": "Skeleton paragraph and H5"
            },
            "S": {
              "$type": "sizing",
              "$value": "{core.fontSize.fluid.step2}",
              "$description": "H4 Skeletons"
            },
            "M": {
              "$type": "sizing",
              "$value": "{core.fontSize.fluid.step2}",
              "$description": "H3 Skeletons"
            },
            "L": {
              "$type": "sizing",
              "$value": "{core.fontSize.fluid.step3}",
              "$description": "H2 Skeletons"
            },
            "XL": {
              "$type": "sizing",
              "$value": "{core.fontSize.fluid.step5}",
              "$description": "H1 Skeletons"
            }
          }
        },
        "adaptiveLarge": {
          "text": {
            "XS": {
              "$type": "sizing",
              "$value": "{global.fontSize.adaptive.step0}",
              "$description": "Skeleton paragraph and H5"
            },
            "S": {
              "$type": "sizing",
              "$value": "{global.fontSize.adaptive.step2}",
              "$description": "H4 Skeletons"
            },
            "M": {
              "$type": "sizing",
              "$value": "{global.fontSize.adaptive.step2}",
              "$description": "H3 Skeletons"
            },
            "L": {
              "$type": "sizing",
              "$value": "{global.fontSize.adaptive.step3}",
              "$description": "H2 Skeletons"
            },
            "XL": {
              "$type": "sizing",
              "$value": "{global.fontSize.adaptive.step5}",
              "$description": "H1 Skeletons"
            }
          }
        }
      },
      "stepflow": {
        "M": {
          "$type": "sizing",
          "$value": "{global.size.micro.M}",
          "$description": "Step height"
        }
      },
      "stepindicator": {
        "bar": {
          "$type": "sizing",
          "$value": "{global.size.micro.XXS}",
          "$description": "step indicator bar"
        },
        "stepcircle": {
          "S": {
            "$type": "sizing",
            "$value": "{global.size.macro.XS}",
            "$description": "M size step indicator"
          },
          "M": {
            "$type": "sizing",
            "$value": "{global.size.macro.S}",
            "$description": "M size step indicator"
          }
        }
      }
    },
    "color": {
      "bg-skeleton": {
        "$type": "color",
        "$value": "{modes.color.status.content.loading}"
      },
      "bg-default": {
        "$type": "color",
        "$value": "{modes.color.interactive.progress.bg}"
      },
      "border-default": {
        "$type": "color",
        "$value": "{modes.color.generic.fg.soft}"
      },
      "fg-ai": {
        "$type": "color",
        "$value": "{modes.color.interactive.ai.default}",
        "$description": "For progress ring in ai buttons"
      },
      "fg-alt": {
        "$type": "color",
        "$value": "{modes.color.status.positive.default}"
      },
      "fg-alt2": {
        "$type": "color",
        "$value": "{modes.color.generic.content.firm}"
      },
      "fg-caution": {
        "$type": "color",
        "$value": "{modes.color.status.caution.default}"
      },
      "fg-default": {
        "$type": "color",
        "$value": "{modes.color.interactive.monochrome.active}"
      },
      "fg-error": {
        "$type": "color",
        "$value": "{modes.color.status.negative.default}"
      },
      "fg-info": {
        "$type": "color",
        "$value": "{modes.color.status.info.default}"
      },
      "label-alt": {
        "$type": "color",
        "$value": "{modes.color.generic.content.firm}"
      },
      "label-default": {
        "$type": "color",
        "$value": "{modes.color.generic.content.harsh}"
      },
      "datavis": {
        "fg-default": {
          "$type": "color",
          "$value": "{modes.color.status.custom.teal.default}"
        }
      },
      "loader": {
        "bg-default": {
          "$type": "color",
          "$value": "{modes.color.interactive.progress.bg}"
        },
        "fg-default": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.active}",
          "$description": "used for loader standard loader spinner"
        },
        "fg-error": {
          "$type": "color",
          "$value": "{modes.color.status.negative.default}"
        },
        "fg-complete": {
          "$type": "color",
          "$value": "{modes.color.status.positive.default}"
        },
        "inverse": {
          "bg-default": {
            "$type": "color",
            "$value": "{modes.color.interactive.progress.bgAlt}",
            "$description": "used for inverse loader bgs"
          },
          "fg-default": {
            "$type": "color",
            "$value": "{modes.color.interactive.monochrome.withActive}",
            "$description": "used on inverse loading spinner"
          }
        }
      },
      "stepflow": {
        "bg-active": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.active}"
        },
        "bg-complete": {
          "$type": "color",
          "$value": "{modes.color.status.positive.default}"
        },
        "bg-default": {
          "$type": "color",
          "$value": "{modes.color.generic.fg.nought}"
        },
        "border-active-inner": {
          "$type": "color",
          "$value": "{modes.color.generic.fg.nought}"
        },
        "border-active-outer": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.active}"
        },
        "border-default": {
          "$type": "color",
          "$value": "{modes.color.generic.fg.soft}"
        },
        "label-alt": {
          "$type": "color",
          "$value": "{modes.color.generic.content.firm}"
        },
        "label-default": {
          "$type": "color",
          "$value": "{modes.color.generic.content.harsh}"
        }
      },
      "stepindicator": {
        "bg-active": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.active}"
        },
        "bg-complete": {
          "$type": "color",
          "$value": "{modes.color.status.positive.default}"
        },
        "border-active-outer": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.active}"
        },
        "border-default": {
          "$type": "color",
          "$value": "{modes.color.generic.fg.soft}"
        },
        "border-success": {
          "$type": "color",
          "$value": "{modes.color.status.positive.default}"
        },
        "label-active": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.withActive}"
        },
        "label-complete": {
          "$type": "color",
          "$value": "{modes.color.interactive.monochrome.withActive}"
        },
        "label-default": {
          "$type": "color",
          "$value": "{modes.color.generic.content.harsh}"
        }
      }
    },
    "opacity": {
      "carousel": {
        "inactive": {
          "$type": "opacity",
          "$value": "0.3",
          "$description": "Inactive slide IMAGE layer."
        }
      }
    }
  }
}